# hpf.conf - default configuration for HPF

<VirtualHost *:80>
  ServerName localhost
  
  #DocumentRoot /var/www
  
  CustomLog /var/log/apache2/hpf.access.log combined
  ErrorLog /var/log/apache2/hpf.error.log

  ProxyPass					/hpfapi				localhost:1337
  ProxyPassReverse			/hpfapi				localhost:1337
</VirtualHost>